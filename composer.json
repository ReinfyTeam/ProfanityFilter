{
    "name": "reinfyteam/profanityfilter",
    "description": "The best profanity filter for chat with api for plugin developers!",
    "type": "project",
    "homepage": "https://github.com/ReinfyTeam",
    "license": "LGPL-2.1",
    "require": {
        "php": "^8.0",
        "pocketmine/pocketmine-mp": "4.12.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "v3.14.4",
        "phpstan/phpstan": "1.10.2",
        "phpstan/phpstan-phpunit": "^1.1.0",
        "phpstan/phpstan-strict-rules": "^1.2.0",
        "phpunit/phpunit": "^9.2"
    },
    "autoload": {
        "psr-4": {
            "ReinfyTeam\\ProfanityFilter\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}